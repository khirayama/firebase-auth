{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_PATH=src ts-node src/index.ts",
    "dev": "NODE_PATH=src nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "tslint --format verbose -c tslint.json 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix --format verbose -c tslint.json 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "format": "prettier --ignore-path ../.gitignore --config .prettierrc --write 'src/**/!(*.json|*.jpg|*.pug|*.html|*.png|*.webp|*.svg)' 'test/**/!(*.json|*.jpg|*.pug|*.html|*.png|*.webp|*.svg)' bin webpack.config.js .eslintrc.js .postcssrc.js",
    "precommit": "npm run lint:fix && npm run format",
    "test": "NODE_PATH=src mocha --require espower-typescript/guess src/**/*.spec.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "express": "4.16.3",
    "pug": "2.0.3"
  },
  "devDependencies": {
    "@types/mocha": "5.2.3",
    "@types/node": "10.5.1",
    "concurrently": "3.6.0",
    "cpx": "1.5.0",
    "cssnano": "3.10.0",
    "espower-typescript": "9.0.0",
    "mocha": "5.2.0",
    "nodemon": "1.17.5",
    "postcss-cli": "5.0.1",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "postcss-simple-vars": "4.1.0",
    "power-assert": "1.6.0",
    "prettier": "1.13.7",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-microsoft-contrib": "5.0.3",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.9.2",
    "webpack": "4.14.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.8"
  }
}
